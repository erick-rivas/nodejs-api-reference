{
  "name": "nodejs-reference",
  "version": "1.0.0",
  "description": "Reference for the development of Node.js APIs",
  "private": true,
  "apidoc": {
    "title": "Node.js Reference",
    "sampleUrl": "http://localhost:4004/v1",
    "order": [
      "get",
      "post",
      "put",
      "delete"
    ]
  },
  "scripts": {
    "gen-api": "tsc && node ./bin/gen-api.js",
    "gen-react": "tsc && node ./bin/gen-react.js",
    "start": "tsc && node ./bin/www",
    "debug": "tsc && node dist/debug.js"
  },
  "dependencies": {
    "@types/node": "11.13.6",
    "@types/request": "2.48.1",
    "archiver": "3.0.0",
    "bcrypt": "3.0.6",
    "body-parser": "1.18.3",
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "csv-parse": "4.4.1",
    "debug": "4.1.1",
    "dotenv": "7.0.0",
    "express": "4.16.4",
    "express-slow-down": "1.3.1",
    "faker": "4.1.0",
    "helmet": "3.16.0",
    "i": "0.3.6",
    "log": "6.0.0",
    "module-alias": "2.2.0",
    "morgan": "1.9.1",
    "multer": "1.4.1",
    "mysql": "2.17.1",
    "request": "2.88.0",
    "serve-favicon": "2.5.0",
    "typescript": "3.4.4"
  },
  "_moduleAliases": {
    "@lt": "dist/src/latest",
    "@support": "dist/src/support",
    "@util": "dist/src/util"
  },
  "nodemonConfig": {
    "watch": [
      "src",
      "routes",
      "docs",
      "app.ts"
    ],
    "ext": "ts"
  }
}
